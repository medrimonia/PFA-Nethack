package bot;

public class Bot {
	
	int dungeonLevel;
	Map map;
	
	public Bot(){
		dungeonLevel = 0;
		map = null;
	}

	public void treatInformation(Information i) {
		switch (i.getVariable()){
		case DUNGEON_LEVEL: dungeonLevel = (Integer)i.getValue(); break;
		case MAP: map = Map(i.getValue()); break;
		}
	}
	
	@Override
	public String toString(){
		//TODO 
		return null;
	}
}

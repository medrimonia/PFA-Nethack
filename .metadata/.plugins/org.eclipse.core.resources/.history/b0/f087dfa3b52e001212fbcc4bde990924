package bot;

public class Map {

	private Square[][] content;

	public Map(char[][] map) {
		content = new Square[map.length][map[0].length];
		for (int line = 0; line < map.length ; line++)
			for (int column = 0; column < map[line].length; column++)
				content[line][column] = new Square(map[line][column]);
	}

}
